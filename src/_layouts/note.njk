---
layout: base.njk
---

<main>
	<article class="Page">
		<header class="Page-header">
			<h1 class="Page-title">{{ title }}</h1>
			<time class="Date" datetime="{{ date | formatDate() }}">{{ date | formatDate('full') }}</time>
		</header>
		<div class="Page-content">{{ content | safe }}</div>
	</article>
</main>
