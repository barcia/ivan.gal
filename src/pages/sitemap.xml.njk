<?xml version="1.0" encoding="utf-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
{%- for entry in collections.all %}
  <url>
    <loc>{{ entry.url | absUrl }}</loc>
    <lastmod>{{ entry.date | formatDate() }}</lastmod>
  </url>
{%- endfor %}
</urlset>
