<ul class="NoteList">
    {% if collections.notes.length > 0 %}
        {%- for post in collections.notes | reverse -%}
        <li>
            <a href="{{ post.url }}">
                <article class="NoteItem">
                    <h2 class="NoteItem-title">{{ post.data.title }}</h2>
                    <time class="Date" datetime="{{ post.date | formatDate() }}">{{ post.date | formatDate('full') }}</time>
                </article>
            </a>
        </li>
        {%- endfor -%}
    {% else %}
        <small>Silence is golden</small>
    {% endif %}
</ul>
