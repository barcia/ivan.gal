{
	"name": "web-starter",
	"version": "20.01.2",
	"license": "MIT",
	"description": "A web starter kit",
	"homepage": "https://www.barcia.dev/web-starter",
	"repository": "https://github.com/barcia/web-starter",
	"private": true,
	"author": "Iv√°n Barcia <ivan@barcia.dev> (https://barcia.dev)",
	"scripts": {
		"prestart": "npm run clean",
		"start": "parcel watch ./src/assets/main.js & eleventy --serve --port=8080",
		"prebuild": "npm run clean",
		"build": "parcel build ./src/assets/main.js --no-content-hash --no-source-maps && eleventy",
		"clean": "rimraf ./dist/",
		"lint": "npm run lint:css; npm run lint:js",
		"lint:css": "stylelint './src/assets/scss'",
		"lint:js": "eslint './src/assets/js'"
	},
	"devDependencies": {
		"@11ty/eleventy": "^0.10.0",
		"@babel/core": "^7.7.7",
		"@babel/preset-env": "^7.7.7",
		"@barcia/stylelint-config-order": "^1.0.0-alpha.3",
		"autoprefixer": "^9.7.3",
		"browserdom": "^3.0.0-beta.3",
		"eslint": "^6.8.0",
		"eslint-config-airbnb-base": "^14.0.0",
		"eslint-plugin-import": "^2.20.0",
		"parcel-bundler": "^1.12.4",
		"rimraf": "^3.0.0",
		"sass": "^1.24.4",
		"standarize": "^4.0.0",
		"stylelint": "^12.0.1",
		"stylelint-config-recommended": "^3.0.0",
		"stylelint-config-standard": "^19.0.0",
		"stylelint-order": "^4.0.0",
		"stylelint-scss": "^3.13.0"
	}
}
